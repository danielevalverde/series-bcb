<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	
	<display-name>series-bcb</display-name>
	<description></description>

	<filter>
		<filter-name>series.wicket</filter-name>
		<filter-class>org.apache.wicket.protocol.http.WicketFilter</filter-class>
		<init-param>
			<param-name>wicket.configuration</param-name>
			<param-value>DEPLOYMENT</param-value>
		</init-param>
		<init-param>
			<param-name>applicationFactoryClassName</param-name>
			<param-value>org.apache.wicket.protocol.http.ContextParamWebApplicationFactory</param-value>
		</init-param>
		<init-param>
			<param-name>applicationClassName</param-name>
			<param-value>br.com.sunlive.series.web.app.SeriesApplication</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>series.wicket</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
</web-app>